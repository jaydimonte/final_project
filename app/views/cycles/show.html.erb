<div class="page-header">
  <h3>Cycle #<%= @cycle.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Start date</dt>
      <dd><%= @cycle.start_date %></dd>

      <dt>End date</dt>
      <dd><%= @cycle.end_date %></dd>

      <dt>User</dt>
      <dd><%= @cycle.user_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/cycles" class="btn btn-primary">
        Back
      </a>
      <a href="/cycles/<%= @cycle.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_cycle/<%= @cycle.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
